{
  "name": "figma-plugin-run-plugin-api",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Ryo Nakae",
  "scripts": {
    "build": "esbuild src/code.ts --bundle --outdir=dist --target=es6 --alias:@=./src --minify --drop:console",
    "check": "biome check",
    "dev": "esbuild src/code.ts --bundle --outdir=dist --target=es6 --alias:@=./src --watch",
    "prepare": "husky"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": ["biome check --write"]
  },
  "dependencies": {
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@figma/plugin-typings": "1.110.0",
    "@types/lodash": "4.14.182",
    "@types/node": "17.0.8",
    "esbuild": "0.25.3",
    "husky": "9.1.7",
    "lint-staged": "15.5.1",
    "typescript": "4.5.4"
  }
}
